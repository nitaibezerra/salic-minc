                function abrirDinamico (elemento, obj) {
                    if ($(elemento).css('display') == 'none') {
                        $(elemento).fadeIn('fast', function () {
                            $('img', $(obj)).attr('src', '<?php echo $this->baseUrl(); ?>/public/img/navigation-baixo.PNG')
